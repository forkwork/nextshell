{
  "server": {
    "compressionLevel": 9
  },
  "logging": {
    "level": "info",
    "format": "combined",
    "httpLogging": false
  },
  "security": {
    "cors": {
      "origin": ["https://nextshell.app", "https://app.nextshell.io"],
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "rateLimit": {
      "windowMs": 900000,
      "max": 60
    },
    "helmet": {
      "contentSecurityPolicy": true,
      "xssFilter": true,
      "hsts": {
        "maxAge": 31536000,
        "includeSubDomains": true,
        "preload": true
      }
    }
  },
  "themes": {
    "previewsEnabled": false
  },
  "workspace": {
    "maxHistoryItems": 500,
    "saveSessionOnExit": true
  },
  "workflows": {
    "autoloadWorkflows": true
  }
}
